{
  "name": "syn",
  "description": "Syn cli",
  "version": "0.0.1",
  "author": "voidp34r",
   "bin": {
    "syn": "./bin/run"
  },
  "bugs": {
    "url": "https://github.com/voidp34r/syn/issues"
  },
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^13"
  },
   "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/voidp34r/syn#readme",
  "keywords": [
    "syn",
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "syn",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voidp34r/syn.git"
  },
  "scripts": {
    "start": "./bin/run syn",
    "debug": "./bin/run syn",
    "watch": "./bin/run syn",
    "lint": "eslint",
    "postpack": "rm -f oclif.manifest.json",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "sn": "snapcraft --debug",
    "deploy": "snapcraft --debug && snapcraft push --release=stable syn_*.snap",
    "deploy:stable": "snapcraft --debug && snapcraft push --release=stable syn_*.snap",
    "version": "oclif-dev readme && git add README.md"
  }
}
